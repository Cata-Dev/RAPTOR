{
  "name": "raptor",
  "version": "0.2.0",
  "description": "RAPTOR typescript/Node.js implementation",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cata-Dev/RAPTOR.git"
  },
  "scripts": {
    "build": "tsc",
    "prettier": "prettier \"**/*.ts\" --write",
    "type-check": "tsc -noEmit",
    "lint": "eslint ./src/ --fix",
    "full-lint": "pnpm run lint && pnpm run type-check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "pnpm run build"
  },
  "author": "Catatomik",
  "license": "MIT",
  "dependencies": {
    "@catatomik/dijkstra": "github:catatomik/dijkstra",
    "@typegoose/typegoose": "^12.2.0",
    "core-js": "^3.36.0",
    "mongodb": "^6.4.0",
    "proj4": "^2.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/proj4": "^2.5.5",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "5.3.3",
    "typescript-eslint": "^7.1.0"
  }
}
