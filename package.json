{
  "name": "raptor",
  "version": "0.2.0",
  "description": "RAPTOR typescript/Node.js implementation",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cata-Dev/RAPTOR.git"
  },
  "scripts": {
    "build": "tsc",
    "prettier": "prettier \"**/*.ts\" --write",
    "type-check": "tsc -noEmit",
    "lint": "eslint ./src/ --ext .ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "full-lint": "npm run lint && npm run type-check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build"
  },
  "author": "Catatomik",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "5.0.4"
  }
}
